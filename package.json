{
  "name": "ts-linked-list",
  "version": "1.0.0",
  "description": "A doubly linked list written in TypeScript",
  "keywords": [
    "doubly linked list",
    "linked list",
    "typescript"
  ],
  "homepage": "https://tuelsch.github.io/ts-linked-list/",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuelsch/ts-linked-list.git"
  },
  "bugs": {
    "url": "https://github.com/tuelsch/ts-linked-list/issues"
  },
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "typings": "dist/LinkedList.d.ts",
  "author": "Philipp Gfeller <tuelsch@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -cw",
    "lint": "tslint --project \".\"",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "doc": "typedoc --theme minimal --target es6 --mode file --out ./doc ./src",
    "preversion": "yarn test",
    "postversion": "git push --tags && npm publish",
    "deploy-patch": "yarn version --patch --force -m \"version %s\""
  },
  "devDependencies": {
    "@types/jest": "^24.0.0",
    "camelcase": "^5.0.0",
    "rollup": "^1.10.1",
    "jest": "^24.6.0",
    "rollup-plugin-commonjs": "^9.2.0",
    "rollup-plugin-node-resolve": "^4.0.0",
    "rollup-plugin-sourcemaps": "^0.4.2",
    "rollup-plugin-typescript2": "^0.20.1",
    "rollup-plugin-uglify": "^6.0.1",
    "ts-jest": "^24.0.1",
    "tslint": "^5.12.0",
    "typedoc": "^0.14.1",
    "typescript": "^3.2.2"
  }
}
