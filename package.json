{
  "name": "ts-linked-list",
  "version": "0.0.5",
  "description": "A doubly linked list written in TypeScript",
  "keywords": [
    "doubly linked list",
    "linked list",
    "typescript"
  ],
  "homepage": "https://tuelsch.github.io/ts-linked-list/",
  "repository": {
    "type": "git",
    "url": "https://github.com/tuelsch/ts-linked-list.git"
  },
  "bugs": {
    "url": "https://github.com/tuelsch/ts-linked-list/issues"
  },
  "main": "dist/index.js",
  "min": "dist/index.min.js",
  "module": "dist/index.es.js",
  "files": [
    "dist"
  ],
  "types": "dist/index.d.ts",
  "author": "Philipp Gfeller <tuelsch@gmail.com>",
  "license": "MIT",
  "private": false,
  "scripts": {
    "build": "rollup -c",
    "dev": "rollup -cw",
    "lint": "tslint --project \".\"",
    "test": "jest --coverage",
    "test:watch": "jest --watchAll",
    "doc": "typedoc --theme minimal --out ./doc ./src",
    "preversion": "yarn test",
    "postversion": "git push --tags && npm publish",
    "deploy-patch": "yarn version --patch --force -m \"version %s\""
  },
  "devDependencies": {
    "@types/jest": "^23.3.11",
    "jest": "^23.6.0",
    "rollup": "^1.0.0",
    "rollup-plugin-typescript2": "^0.18.1",
    "rollup-plugin-uglify": "^6.0.1",
    "ts-jest": "^23.10.5",
    "tslint": "^5.12.0",
    "typedoc": "^0.14.1",
    "typescript": "^3.2.2"
  }
}
